<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:calendardatepicker="http://schemas.android.com/apk/res-auto">

    <item android:drawable="@drawable/calendar_date_picker_cell_pressed_round_trip" calendardatepicker:state_cell_selection_first="true" calendardatepicker:state_cell_selection_last="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_pressed_start" calendardatepicker:state_cell_in_range="true" calendardatepicker:state_cell_selection_first="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_selected" calendardatepicker:state_cell_in_range="false" calendardatepicker:state_cell_selection_first="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_pressed_end" calendardatepicker:state_cell_selection_last="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_selected" calendardatepicker:state_cell_selected="true"/>
    <item android:drawable="@drawable/bg_calendar_cell_today" calendardatepicker:state_cell_today="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_disabled" calendardatepicker:state_cell_outside_of_valid_date_range="true"/>
    <item android:drawable="@drawable/calendar_date_picker_cell_normal"/>

</selector>
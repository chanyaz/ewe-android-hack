    <string name="checkout_payment_options">Kreditkort, betala med MrJet Rewards</string>

    <!-- VIP strings -->
    <string name="vip_loyalty_applied_message">Rewards tillämpade!</string>
    <!-- String to be appended with VIP in header of room rates. -->
    <string name="vip_loyalty_applied_header_message">@string/vip_loyalty_applied_message</string>
    <string name="vip_loyalty_applied_map_message">&lt;b&gt;VIP&lt;/b&gt; Rewards tillämpade!</string>
    <!-- Title for VIP Access message -->
    <string name="vip_access">VIP</string>
    <!-- Message that explains the meaning of VIP Access -->
    <string name="vip_access_message">På VIP-hotell får Guld- och Platina-medlemmar gratis rumsuppgraderingar och andra förmåner som är tillgängliga vid incheckning.</string>
    <!-- Filter for showing only VIP access hotels -->
    <string name="filter_vip_access">Endast VIP-hotell</string>
    <!-- Title for VIP Access message for Hotel Results -->
    <string name="vip">VIP</string>

    <!-- Shop with points -->
    <string name="regular_loyalty_applied_message">MrJet Rewards tillämpade!</string>
    <!-- String to be displayed on switching SWP off -->
    <string name="swp_off_widget_header">Tillämpa MrJet Rewards</string>
    <!-- String to be displayed on switching SWP on -->
    <string name="swp_on_widget_header">MrJet Rewards tillämpade</string>
    <!-- Available loyalty points -->
    <string name="available_loyalty_points">Tillgängliga MrJet Rewards</string>
    <!-- SwP Widget you have points template. points_or_amount = it may have money (in case of Expedia) or point value (in case of MrJet Rewards). E.g. You have $30 in MrJet Rewards -->
    <string name="swp_widget_points_value_TEMPLATE">Du har {points_or_amount} i MrJet Rewards</string>
    <!-- Label indicating MrJet Rewards used by the user -->
    <string name="hotel_cost_breakdown_burn_points_TEMPLATE">Använda MrJet Rewards</string>

    <string name="loyalty_applied_excluding_taxes_and_fees_message">MrJet Rewards tillämpade! (Skatter och avgifter ingår ej)</string>
    <string name="youll_earn_points_TEMPLATE">Du tjänar {reward_currency} i MrJet Rewards med den här bokningen!</string>
    <string name="x_rewards_currency_for_this_trip_TEMPLATE">Du tjänar {reward_currency} i MrJet Rewards med den här resan!</string>

    <!-- Label indicating MrJet Rewards Available. Eg: $1000.12 in MrJet Rewards available-->
    <string name="bucks_available_TEMPLATE">{money} i MrJet Rewards tillgängliga</string>
    <!-- Label indicating MrJet Rewards Applied By User. Eg: $1000.12 in MrJet Rewards applied-->
    <string name="bucks_applied_TEMPLATE">{money} i MrJet Rewards tillämpade</string>
    <!-- Label above the slider when user is fully paying with MrJet Rewards. Eg: You are using $1000.12 in MrJet Rewards-->
    <string name="you_are_using_bucks_TEMPLATE">Du använder {amount} i MrJet Rewards</string>
    <!-- Label above the slider when user is paying with MrJet Rewards and Card. Eg: You are using $1000.12 in MrJet Rewards\nYour card will be charged $100.00-->
    <string name="payment_through_card_and_bucks_TEMPLATE">Du använder {amount} i MrJet Rewards\nDitt kort kommer att debiteras {dueamount}</string>

    <!-- Rewards -->
    <!-- Long name for rewards base tier; note that brand name is not parameterized here as other brands will need to completely override this string -->
    <string name="reward_base_tier_name_long">Silvermedlem</string>

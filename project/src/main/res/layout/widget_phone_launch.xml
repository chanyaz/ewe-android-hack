<?xml version="1.0" encoding="utf-8"?><!--NOTE: there is duplicate layout for testing without SVG custom view from air attach banner-->
<com.expedia.bookings.launch.widget.PhoneLaunchWidget
    android:id="@+id/new_phone_launch_widget"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/launch_list_bg"
    android:clipChildren="false"
    android:clipToPadding="false">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/pro_wizard_search_bar"
            layout="@layout/pro_wizard_search_bar_view" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <include
                android:id="@+id/launch_list_widget"
                layout="@layout/widget_launch_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:id="@+id/pro_wizard_search_bar_shadow"
                android:layout_width="match_parent"
                android:layout_height="@dimen/toolbar_dropshadow_height"
                android:background="@drawable/tb_shadow" />

        </FrameLayout>

    </LinearLayout>

    <View
        android:id="@+id/darkness"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.0"
        android:background="#000000" />

    <FrameLayout
        android:id="@+id/lob_view_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/new_launch_toolbar_background_color"
        android:paddingBottom="@dimen/card_view_container_margin"
        android:visibility="visible" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="@dimen/new_launch_fab_width"
        android:layout_height="@dimen/new_launch_fab_height"
        android:layout_gravity="bottom|right|end"
        android:layout_marginBottom="@dimen/new_launch_screen_fab_bottom_margin"
        android:layout_marginRight="@dimen/new_launch_screen_fab_bottom_margin"
        android:contentDescription="@string/search"
        android:src="@drawable/fab_search"
        android:visibility="invisible"
        app:backgroundTint="@color/new_launch_fab_color" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/chat"
        android:layout_width="@dimen/new_launch_fab_width"
        android:layout_height="@dimen/new_launch_fab_height"
        android:layout_gravity="bottom|left|end"
        android:layout_marginBottom="@dimen/new_launch_screen_fab_bottom_margin"
        android:layout_marginLeft="@dimen/new_launch_screen_fab_bottom_margin"
        android:contentDescription="Start Chat"
        android:src="@drawable/ic_share_short"
        android:visibility="visible"
        app:backgroundTint="@color/white" />

    <include
        android:id="@+id/launch_error"
        layout="@layout/section_launch_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/launch_lob_height"
        android:visibility="gone" />

    <View
        android:id="@+id/toolbar_dropshadow"
        android:layout_width="match_parent"
        android:layout_height="@dimen/toolbar_dropshadow_height"
        android:alpha="0.0"
        android:background="@drawable/tb_shadow" />

</com.expedia.bookings.launch.widget.PhoneLaunchWidget>
<?xml version="1.0" encoding="utf-8"?>
<com.expedia.bookings.presenter.CarsPresenter xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cars_presenter"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/widget_car_params"
        layout="@layout/widget_car_search_params"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/widget_car_progress_indicator"
        layout="@layout/widget_car_progress"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/car_category_list"
        layout="@layout/widget_car_category_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/car_details"
        layout="@layout/widget_category_details"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include
        android:id="@+id/car_checkout"
        layout="@layout/widget_car_checkout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</com.expedia.bookings.presenter.CarsPresenter>
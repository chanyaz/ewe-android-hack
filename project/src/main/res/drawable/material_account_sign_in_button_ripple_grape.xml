        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="#40000000" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="#40000000" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="@color/material_checkout_account_button_background_grape_color" />
        </shape>
    </item>
</selector>

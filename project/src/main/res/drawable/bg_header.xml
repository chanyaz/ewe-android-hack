<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="rectangle">
			<solid
				android:color="#454545" />
		</shape>
	</item>
	<item
		android:top="1px">
		<shape
			android:shape="rectangle">
			<gradient
				android:startColor="#303030"
				android:endColor="#0e0e0e"
				android:angle="270" />
		</shape>
	</item>
</layer-list>
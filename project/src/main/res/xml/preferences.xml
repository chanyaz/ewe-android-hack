<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory
		android:layout="@layout/preference_category_title"
		android:title="@string/preference_category_general">
        <com.expedia.bookings.dialog.DomainPreference
            android:key="@string/PointOfSaleKey"
			android:layout="@layout/preference"
            android:title="@string/preference_point_of_sale_title" />
		<com.expedia.bookings.dialog.ClearPrivateDataDialogPreference
			android:key="@string/preference_clear_private_data_key"
			android:layout="@layout/preference"
			android:title="@string/clear_private_data"
			android:dialogTitle="@string/dialog_clear_private_data_title"
			android:dialogMessage="@string/dialog_sign_out_and_clear_private_data_msg"
			android:positiveButtonText="@string/ok"
			android:negativeButtonText="@string/cancel" />
	</PreferenceCategory>
</PreferenceScreen>

	<!-- Rules & restrictions section header -->
	<string name="rules_restrictions">Peraturan &amp; Sekatan</string>
	<!-- Disclaimer about how to earn expedia points -->
	<string name="disclaimer_expedia_points">Jika anda mempunyai akaun Expedia Rewards, sila tempah menggunakan alamat e-mel yang sama dan mata anda akan dimasukkan secara automatik selepas penginapan anda. Sila maklum bahawa ia mungkin mengambil sehingga 30 hari untuk mata anda muncul dalam akaun.</string>
	<!-- Message at the bottom of the booking screen with how much you will be charged -->
	<string name="charge_details_template">Kad kredit anda akan dicaj %s</string>
	<!-- Button label for confirming your booking -->
	<string name="confirm_book">Sahkan &amp; Tempah</string>
	<!-- Error message if field was required but not entered -->
	<string name="required_field">Sila masukkan nilai untuk medan ini.</string>
	<!-- Error message if field had data entered but was invalid -->
	<string name="invalid_field">Sila masukkan nilai yang sah untuk medan ini.</string>
	<!-- Error if an invalid card number was entered -->
	<string name="invalid_card_number">Nombor kad kredit yang anda masukkan tidak sah, sila cuba lagi.</string>
	<!-- Error if an invalid credit card type was entered -->
	<string name="invalid_card_type">Kami tidak dapat mengesan jenis kad kredit anda, sila masukkan kad kredit yang sah.</string>
	<!-- Error if an invalid month (not 1-12) was entered -->
	<string name="invalid_month">Sila masukkan nilai antara 1 dan 12</string>
	<!-- Error if an invalid year was entered -->
	<string name="invalid_expiration_year">Sila masukkan tahun yang masih belum berlalu</string>
	<!-- Error if an invalid security code was entered -->
	<string name="invalid_security_code">Sila masukkan kod keselamatan tiga atau empat digit</string>
	<!-- Error if an invalid currency code is being used with American Express -->
	<string name="invalid_currency_for_amex">Maaf, pembayaran dalam mata wang ini (%s) tidak disokong oleh American Express. Sila gunakan kad kredit lain.</string>
	<!-- Error if user agreement needs to be checked but is not -->
	<string name="error_no_user_agreement">Anda mesti menerima terma untuk membuat tempahan</string>
	<!-- Message to display to user while booking is in progress -->
	<string name="booking_loading">Sedang Menempah...</string>
	<!-- Title for error dialog when booking fails -->
	<string name="error_booking_title">Ralat</string>
	<!-- Error message when we couldn't get a response -->
	<string name="error_booking_null">Tidak dapat menyambung kepada pelayan Expedia. Sila semak sambungan anda dan cuba lagi.</string>
	

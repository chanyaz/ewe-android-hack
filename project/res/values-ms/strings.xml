<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<string name="app_name">Expedia</string>
	
	<!-- Strings used a lot of places -->
	<!-- A number of adults -->
	<plurals name="number_of_adults">
		<item quantity="zero">%d Dewasa</item>
		<item quantity="one">%d Dewasa</item>
		<item quantity="two">%d Dewasa</item>
		<item quantity="many">%d Dewasa</item>
		<item quantity="other">%d Dewasa</item>
	</plurals>
	<!-- A number of children -->
	<plurals name="number_of_children">
		<item quantity="zero">%d Kanak-kanak</item>
		<item quantity="one">%d Kanak-kanak</item>
		<item quantity="two">%d Kanak-kanak</item>
		<item quantity="many">%d Kanak-kanak</item>
		<item quantity="other">%d Kanak-kanak</item>
	</plurals>

	<!-- Search Activity -->
	<string name="search">Cari</string>
	<!--  The string that appears at the top of the search header.
	      For example: "Chicago, IL - Aug 24 to Aug 25, 2011"
	      1$ - The search address
	      2$ - The check-in month 
	      3$ - The check-in day of month
	      4$ - The check-out month 
	      5$ - The check-out day of month
	      6$ - The year -->
	<string name="booking_info_template">%1$s — %2$s %3$d hingga %4$s %5$s, %6$d</string>
	<!-- Label to show only hotels highly rated by TripAdvisor -->
	<string name="tripadvisor_rating_high">Tunjukkan hotel dengan penarafan tinggi sahaja</string>
	<!-- Label to show hotels with any TripAdvisor rating -->
	<string name="tripadvisor_rating_all">Tunjukkan semua hotel</string>
	<!-- Label indicating that there are no results matching their current filter -->
	<string name="no_filter_results">Maaf, tiada hotel yang sepadan dengan tapisan semasa anda.</string>
	<!-- Label indicating that geolocation failed -->
	<string name="geolocation_failed">Tidak dapat menentukan lokasi carian.</string>
	<!-- Button label for upgrading the app -->
	<string name="upgrade">Tingkatkan</string>
	<!-- Button label for searching the visible map area -->
	<string name="map_search_button">Cari Kawasan Peta</string>
	<!-- Message when app is loading saved search results -->
	<string name="loading_previous">Memuatkan hasil carian terdahulu...</string>
	
	<!-- Price range template (e.g., "($50 - $100)" or "(50CA - 100CA)"). -->
	<string name="price_range_template">(%1$s — %2$s)</string>
	
	<!-- Search pulldown -->
	<!-- Filter hotels within 2 miles away -->
	<string name="filter_2miles">3.22 km</string>
	<!-- Filter hotels within 5 miles away -->
	<string name="filter_5miles">8.05 km</string>
	<!-- Filter hotels within 10 miles away -->
	<string name="filter_10miles">16.09 km</string>
	<!-- Filter setting to show all for that filter setting (e.g., "show all hotels in range") -->
	<string name="all">Semua</string>
	<!-- Sort hotels label -->
	<string name="sort">Susun</string>
	<!-- Sort hotels by popular (the order that Expedia thinks is best) -->
	<string name="popular">Popular</string>
	<!-- Sort hotels by price - also label for "filter by price" section -->
	<string name="price">Harga</string>
	
	<!-- Progress messages -->	
	<!-- Message displayed when app is trying to find where it is (via network/GPS) -->
	<string name="progress_finding_location">Mencari Lokasi Semasa…</string>
	<!-- Message displayed when app is searching a location for hotels -->
	<string name="progress_searching_hotels">Mencari Hotel...</string>
	<!-- Message displayed when search comes up with zero results -->
	<string name="progress_search_failed">Tiada Hotel Ditemui</string>
	
	<!-- Suggestion adapter -->
	<!-- Suggestion to search the user's current location (instead of entering a specific place) -->
	<string name="current_location">Lokasi Semasa</string>
	<!-- Suggestion to search the currently visible map area on the screen -->
	<string name="visible_map_area">Kawasan Peta yang Kelihatan</string>

	<!-- Search list adapter -->
	<!-- The label for how much hotels cost - "from $120/night", for example -->
	<string name="from">Dari</string>
	<!-- The label indicating that this hotel is highly rated on TripAdvisor -->
	<string name="highly_rated">Penarafan Tinggi</string>
	<!-- The label for how much hotels cost, with a strikethrough non-sale price (e.g., "From $120" with the price strikethrough") -->
	<string name="from_template">Dari <strike>%s</strike></string>
	<!-- The label for price per night - "from $120/night", for example -->
	<string name="per_night">/malam</string>
	<!-- Label for red "sale" corner -->
	<string name="sale_caps">JUALAN MURAH</string>


	<string name="star_rating">Penarafan Bintang</string>
	<!-- Label for the hotel's trip advisor rating  -->
	<string name="trip_advisor">TripAdvisor</string>
	<!-- Label for the address section of the description  -->
	<string name="address">Alamat</string>
	<!-- Bullet point for list items  -->
	<string name="bullet_point">•</string>


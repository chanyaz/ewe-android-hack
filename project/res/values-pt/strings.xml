	<string name="invalid_field">Insira um valor válido para este campo.</string>
	<!-- Error if an invalid card number was entered -->
	<string name="invalid_card_number">O número de cartão de crédito que você inseriu não é válido, tente novamente.</string>
	<!-- Error if an invalid credit card type was entered -->
	<string name="invalid_card_type">Não foi possível identificar o tipo do seu cartão de crédito, insira um cartão de crédito válido.</string>

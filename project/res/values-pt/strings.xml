	<!-- Search Activity -->
	<string name="search">Pesquisar</string>
	<!--  The string that appears at the top of the search header.
	      For example: "Chicago, IL - Aug 24 to Aug 25, 2011"
	      1$ - The search address
	      2$ - The check-in month 
	      3$ - The check-in day of month
	      4$ - The check-out month 
	      5$ - The check-out day of month
	      6$ - The year -->
	<string name="booking_info_template">%1$s — de %3$d %2$s a %5$s %4$s, %6$d</string>
	<!-- Label to show only hotels highly rated by TripAdvisor -->
	<string name="tripadvisor_rating_high">Exibir apenas os hotéis mais bem avaliados</string>
	<!-- Label to show hotels with any TripAdvisor rating -->
	<string name="tripadvisor_rating_all">Exibir todos os hotéis</string>
	<!-- Label indicating that there are no results matching their current filter -->
	<string name="no_filter_results">Desculpe, mas nenhum hotel corresponde aos seus filtros atuais.</string>
	<!-- Label indicating that geolocation failed -->
	<string name="geolocation_failed">Não foi possível determinar a posição de pesquisa.</string>
	<!-- Button label for upgrading the app -->
	<string name="upgrade">Nova Versão</string>
	<!-- Button label for searching the visible map area -->
	<string name="map_search_button">Pesquisar na Área do Mapa</string>
	<!-- Message when app is loading saved search results -->
	<string name="loading_previous">Carregando os resultados de pesquisa anteriores...</string>
	
	<!-- Price range template (e.g., "($50 - $100)" or "(50CA - 100CA)"). -->
	<string name="price_range_template">(%1$s — %2$s)</string>
	
	<!-- Progress messages -->	
	<!-- Message displayed when app is trying to find where it is (via network/GPS) -->
	<string name="progress_finding_location">Procurando a Posição Atual...</string>
	<!-- Message displayed when app is searching a location for hotels -->
	<string name="progress_searching_hotels">Procurando Hotéis...</string>
	<!-- Message displayed when search comes up with zero results -->
	<string name="progress_search_failed">Nenhum Hotel Encontrado</string>
	
	<!-- Suggestion adapter -->
	<!-- Suggestion to search the user's current location (instead of entering a specific place) -->
	<string name="current_location">Posição Atual</string>
	<!-- Suggestion to search the currently visible map area on the screen -->
	<string name="visible_map_area">Área Visível do Mapa</string>

	<!-- Search list adapter -->
	<!-- The label for how much hotels cost - "from $120/night", for example -->
	<string name="from">A partir de</string>
	<!-- The label indicating that this hotel is highly rated on TripAdvisor -->
	<string name="highly_rated">Mais Bem Avaliado</string>

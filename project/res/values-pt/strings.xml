	<string name="from_template">A partir de <strike>%s</strike></string>
	<!-- The label for price per night - "from $120/night", for example -->
	<string name="per_night">/noite</string>
	<!-- Label for red "sale" corner -->
	<string name="sale_caps">PROMOÇÃO</string>


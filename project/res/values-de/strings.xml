		<item quantity="two">%d Erwachsene</item>
		<item quantity="many">%d Erwachsene</item>
		<item quantity="other">%d Erwachsene</item>
	</plurals>
	<!-- A number of children -->
	<plurals name="number_of_children">
		<item quantity="zero">%d Kinder</item>

	<!-- Search Activity -->
	<string name="search">Suche</string>
	<!--  The string that appears at the top of the search header.
	      For example: "Chicago, IL - Aug 24 to Aug 25, 2011"
	      1$ - The search address
	      2$ - The check-in month 
	      3$ - The check-in day of month
	      4$ - The check-out month 
	      5$ - The check-out day of month
	      6$ - The year -->
	<string name="booking_info_template">%1$s — %3$d %2$s bis %5$s %4$s %6$d</string>
	<!-- Label to show only hotels highly rated by TripAdvisor -->
	<string name="tripadvisor_rating_high">Nur gut bewertete Hotels anzeigen</string>
	<!-- Label to show hotels with any TripAdvisor rating -->
	<string name="tripadvisor_rating_all">Alle Hotels anzeigen</string>
	<!-- Label indicating that there are no results matching their current filter -->
	<string name="no_filter_results">Leider wurden für Ihre Auswahl keine Hotels gefunden. </string>
	<!-- Label indicating that geolocation failed -->
	<string name="geolocation_failed">Das Ziel wurde nicht gefunden. </string>
	<!-- Button label for upgrading the app -->
	<string name="upgrade">Upgrade</string>
	<!-- Button label for searching the visible map area -->
	<string name="map_search_button">Auf der Karte suchen</string>
	<!-- Message when app is loading saved search results -->
	<string name="loading_previous">Vorherige Suchergebnisse werden geladen ...</string>
	
	<!-- Price range template (e.g., "($50 - $100)" or "(50CA - 100CA)"). -->
	<string name="price_range_template">(%1$s — %2$s)</string>
	
	<!-- Progress messages -->	
	<!-- Message displayed when app is trying to find where it is (via network/GPS) -->
	<string name="progress_finding_location">Suche nach dem Standort erfolgt...</string>
	<!-- Message displayed when app is searching a location for hotels -->
	<string name="progress_searching_hotels">Suche nach Hotels erfolgt...</string>
	<!-- Message displayed when search comes up with zero results -->
	<string name="progress_search_failed">Keine Hotels gefunden.</string>
	
	<!-- Suggestion adapter -->
	<!-- Suggestion to search the user's current location (instead of entering a specific place) -->
	<string name="current_location">Aktueller Standort</string>
	<!-- Suggestion to search the currently visible map area on the screen -->
	<string name="visible_map_area">Karte</string>

	<!-- Search list adapter -->
	<!-- The label for how much hotels cost - "from $120/night", for example -->
	<string name="from">Ab</string>

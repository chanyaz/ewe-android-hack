<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<string name="app_name">Expedia</string>
	
	<!-- Strings used a lot of places -->
	<!-- A number of adults -->
	<plurals name="number_of_adults">
		<item quantity="zero">%d Dewasa</item>
		<item quantity="one">%d Dewasa</item>
		<item quantity="two">%d Dewasa</item>
		<item quantity="many">%d Dewasa</item>
		<item quantity="other">%d Dewasa</item>
	</plurals>
	<!-- A number of children -->
	<plurals name="number_of_children">
		<item quantity="zero">%d Anak</item>
		<item quantity="one">%d Anak</item>
		<item quantity="two">%d Anak</item>
		<item quantity="many">%d Anak</item>
		<item quantity="other">%d Anak</item>

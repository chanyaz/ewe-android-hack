	<string name="required_field">Isi kolom ini.</string>
	<!-- Error message if field had data entered but was invalid -->
	<string name="invalid_field">Isi kolom ini dengan nilai yang valid.</string>
	<!-- Error if an invalid card number was entered -->
	<string name="invalid_card_number">Nomor kartu kredit yang Anda masukkan tidak valid, coba lagi.</string>
	<!-- Error if an invalid credit card type was entered -->
	<string name="invalid_card_type">Kami tidak dapat mendeteksi jenis kartu kredit Anda, masukkan kartu kredit yang valid.</string>
	<!-- Error if an invalid month (not 1-12) was entered -->
	<string name="invalid_month">Masukkan nilai antara 1 hingga 12</string>
	<!-- Error if an invalid year was entered -->
	<string name="invalid_expiration_year">Masukkan tahun yang belum lampau</string>
	<!-- Error if an invalid security code was entered -->
	<string name="invalid_security_code">Masukkan kode keamanan tiga atau empat digit</string>

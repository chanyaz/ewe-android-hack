<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<string name="app_name">Expedia</string>
	
	<!-- Strings used a lot of places -->
	<!-- A number of adults -->
	<plurals name="number_of_adults">
		<item quantity="zero">%d adultes</item>
		<item quantity="one">%d adulte</item>
		<item quantity="two">%d adultes</item>
		<item quantity="many">%d adultes</item>
		<item quantity="other">%d adultes</item>
	</plurals>
	<!-- A number of children -->
	<plurals name="number_of_children">
		<item quantity="zero">%d enfants</item>
		<item quantity="one">%d enfant</item>
		<item quantity="two">%d enfants</item>
		<item quantity="many">%d enfants</item>

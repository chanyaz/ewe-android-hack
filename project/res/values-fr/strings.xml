	<string name="security_code_tip_back">Voir au verso de la carte de paiement</string>
	<!-- Rules &amp; restrictions section header -->
	<string name="rules_restrictions">Règles et restrictions</string>
	<!-- Disclaimer about how to earn expedia points -->
	<string name="disclaimer_expedia_points">Si vous disposez d\'un compte Expedia Rewards, veuillez réserver avec la même adresse email et vos points seront automatiquement crédités après votre séjour. Veuillez noter que vos points apparaîtront dans un délai de 30 jours.</string>
	<!-- Message at the bottom of the booking screen with how much you will be charged -->
	<string name="charge_details_template">Votre carte de paiement sera débitée de %s</string>
	<!-- Button label for confirming your booking -->
	<string name="confirm_book">Confirmez &amp; Réservez</string>
	<!-- Error message if field was required but not entered -->
	<string name="required_field">Veuillez saisir une valeur pour ce champ.</string>
	<!-- Error message if field had data entered but was invalid -->
	<string name="invalid_field">Veuillez saisir une valeur valide pour ce champ.</string>
	<!-- Error if an invalid card number was entered -->
	<string name="invalid_card_number">Le numéro de carte de paiement que vous avez saisi n\'est pas valide, veuillez réessayer.</string>
	<!-- Error if an invalid credit card type was entered -->
	<string name="invalid_card_type">Nous n\'avons pas pu identifier le type de carte de paiement, veuillez entrer une carte de paiement valide.</string>
	<!-- Error if an invalid month (not 1-12) was entered -->
	<string name="invalid_month">Veuillez saisir une valeur entre 1 et 12</string>
	<!-- Error if an invalid year was entered -->
	<string name="invalid_expiration_year">Veuillez saisir une année qui n\'est pas déjà écoulée</string>
	<!-- Error if an invalid security code was entered -->
	<string name="invalid_security_code">Veuillez saisir un code de sécurité de 3 ou 4 chiffres</string>
	<!-- Error if an invalid currency code is being used with American Express -->
	<string name="invalid_currency_for_amex">Désolé, les paiements dans cette monnaie (%s) ne sont pas supportés par American Express. Veuillez utiliser une carte de paiement différente.</string>
	<!-- Error if user agreement needs to be checked but is not -->
	<string name="error_no_user_agreement">Vous devez accepter les termes pour pouvoir réserver</string>
	<!-- Message to display to user while booking is in progress -->
	<string name="booking_loading">Réservation...</string>
	<!-- Title for error dialog when booking fails -->
	<string name="error_booking_title">Erreur</string>
	<!-- Error message when we couldn't get a response -->
	<string name="error_booking_null">Impossible de se connecter aux serveurs Expedia. Veuillez vérifier votre connexion et réessayer plus tard.</string>
	
	<!-- Confirmation activity -->

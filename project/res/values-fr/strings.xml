	</plurals>

	<!-- Search Activity -->
	<string name="search">Recherchez</string>
	<!--  The string that appears at the top of the search header.
	      For example: "Chicago, IL - Aug 24 to Aug 25, 2011"
	      1$ - The search address
	      2$ - The check-in month 
	      3$ - The check-in day of month
	      4$ - The check-out month 
	      5$ - The check-out day of month
	      6$ - The year -->
	<string name="booking_info_template">%1$s — du %3$d %2$s au %5$s %4$s %6$d</string>
	<!-- Label to show only hotels highly rated by TripAdvisor -->
	<string name="tripadvisor_rating_high">Afficher uniquement les hôtels les mieux notés</string>
	<!-- Label to show hotels with any TripAdvisor rating -->
	<string name="tripadvisor_rating_all">Afficher tous les hôtels </string>
	<!-- Label indicating that there are no results matching their current filter -->
	<string name="no_filter_results">Aucun hôtel sur Expedia ne correspond aux critères demandés.</string>
	<!-- Label indicating that geolocation failed -->
	<string name="geolocation_failed">Impossible de déterminer la position de recherche.</string>
	<!-- Button label for upgrading the app -->
	<string name="upgrade">Mettre à jour</string>
	<!-- Button label for searching the visible map area -->
	<string name="map_search_button">Chercher sur la carte</string>
	<!-- Message when app is loading saved search results -->
	<string name="loading_previous">Chargement des résultats de la recherche précédente...</string>
	
	<!-- Price range template (e.g., "($50 - $100)" or "(50CA - 100CA)"). -->
	<string name="price_range_template">(%1$s — %2$s)</string>
	
	<!-- Progress messages -->	
	<!-- Message displayed when app is trying to find where it is (via network/GPS) -->
	<string name="progress_finding_location">Recherche de la position actuelle...</string>
	<!-- Message displayed when app is searching a location for hotels -->
	<string name="progress_searching_hotels">Recherche d\'hôtels en cours...</string>
	<!-- Message displayed when search comes up with zero results -->
	<string name="progress_search_failed">Aucun hôtel trouvé sur Expedia</string>
	
	<!-- Suggestion adapter -->
	<!-- Suggestion to search the user's current location (instead of entering a specific place) -->
	<string name="current_location">Position actuelle</string>
	<!-- Suggestion to search the currently visible map area on the screen -->
	<string name="visible_map_area">Carte</string>

	<!-- Search list adapter -->
	<!-- The label for how much hotels cost - "from $120/night", for example -->
	<string name="from">Dès</string>
	<!-- The label indicating that this hotel is highly rated on TripAdvisor -->
	<string name="highly_rated">Recommandé</string>
	<!-- The label for how much hotels cost, with a strikethrough non-sale price (e.g., "From $120" with the price strikethrough") -->
	<string name="from_template">Dès <strike>%s</strike></string>
	<!-- The label for price per night - "from $120/night", for example -->
	<string name="per_night">/nuit</string>
	<!-- Label for red "sale" corner -->
	<string name="sale_caps">OFFRE</string>

	<!-- Hotel Activity -->
	<!-- Label for button to book the hotel -->
	<string name="book_now">Réservez</string>
	<!-- Label for the hotel's star rating -->
	<string name="star_rating">Nombre d\'étoiles</string>
	<!-- Label for the hotel's trip advisor rating  -->
	<string name="trip_advisor">TripAdvisor</string>
	<!-- Label for the address section of the description  -->
	<string name="address">Adresse</string>
	<!-- Bullet point for list items  -->
	<string name="bullet_point">•</string>

	<!-- Shortened Amenity strings -->
	<!-- Internet amenity -->
	<string name="amenity_internet">Internet</string>
	<!-- Pets allowed amenity -->
	<string name="amenity_pets">Animaux</string>


	<string name="hint_expiration_year">aa</string>
	<!-- Security code label -->
	<string name="security_code">Code de sécurité</string>
	<!-- Security code tip (if code is on unknown side of card) -->
	<string name="security_code_tip_front_back">Voir au recto ou au verso de la carte de paiement</string>
	<!-- Security code tip (if code is on front of card) -->
	<string name="security_code_tip_front">Voir au recto de la carte de paiement</string>

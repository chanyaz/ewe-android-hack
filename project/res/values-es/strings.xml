	<!-- Search Activity -->
	<string name="search">Buscar</string>
	<!--  The string that appears at the top of the search header.
	      For example: "Chicago, IL - Aug 24 to Aug 25, 2011"
	      1$ - The search address
	      2$ - The check-in month 
	      3$ - The check-in day of month
	      4$ - The check-out month 
	      5$ - The check-out day of month
	      6$ - The year -->
	<string name="booking_info_template">%1$s — %2$s %3$d a %4$s %5$s, %6$d</string>
	<!-- Label to show only hotels highly rated by TripAdvisor -->
	<string name="tripadvisor_rating_high">Ver solo los hoteles mejor valorados</string>
	<!-- Label to show hotels with any TripAdvisor rating -->
	<string name="tripadvisor_rating_all">Ver todos los hoteles</string>
	<!-- Label indicating that there are no results matching their current filter -->
	<string name="no_filter_results">No hay hoteles que coincidan con los filtros seleccionados.</string>
	<!-- Label indicating that geolocation failed -->
	<string name="geolocation_failed">No es posible determinar la ubicación de búsqueda</string>
	<!-- Button label for upgrading the app -->
	<string name="upgrade">Actualizar</string>
	<!-- Button label for searching the visible map area -->
	<string name="map_search_button">Área de búsqueda del mapa</string>
	<!-- Message when app is loading saved search results -->
	<string name="loading_previous">Cargando los resultados de la búsqueda anterior...</string>
	
	<!-- Price range template (e.g., "($50 - $100)" or "(50CA - 100CA)"). -->
	<string name="price_range_template">(%1$s — %2$s)</string>
	
	<!-- Progress messages -->	
	<!-- Message displayed when app is trying to find where it is (via network/GPS) -->
	<string name="progress_finding_location">Buscando ubicación actual...</string>

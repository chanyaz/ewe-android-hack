	<string name="progress_searching_hotels">Buscando hoteles...</string>
	<!-- Message displayed when search comes up with zero results -->
	<string name="progress_search_failed">No se encontró ningún hotel</string>
	
	<!-- Suggestion adapter -->
	<!-- Suggestion to search the user's current location (instead of entering a specific place) -->
	<string name="current_location">Ubicación actual</string>

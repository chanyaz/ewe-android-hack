<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory android:title="Development">
		<CheckBoxPreference
			android:key="@string/preference_use_dev_api"
			android:title="Use Dev API (requires VPN)"
			android:summaryOff="Don't do it!"
			android:summaryOn="Do it!!!"
			android:defaultValue="false" />
		<CheckBoxPreference
			android:key="@string/preference_proxy_server_enabled"
			android:title="Use Local Proxy Server"
			android:summary="e.g., E3MockProxy built by Shawn"
			android:defaultValue="false" />
		<EditTextPreference
			android:key="@string/preference_proxy_server_address"
			android:title="Proxy Server Address"
			android:summary="The address of your mock proxy server (e.g., 192.168.0.5:3000)"
			android:defaultValue="localhost:3000" />
	</PreferenceCategory>
</PreferenceScreen>

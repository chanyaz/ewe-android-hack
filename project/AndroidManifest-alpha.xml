<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.expedia.bookings" >

    <uses-permission android:name="android.permission.READ_LOGS" />


    <!-- Permissions: For EBad Alpha builds -->
    <permission
        android:name="com.expedia.bookings.alpha.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.expedia.bookings.alpha.permission.MAPS_RECEIVE" />
    <!-- For Push Notifications (GCM) -->
    <permission
        android:name="com.expedia.bookings.alpha.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.expedia.bookings.alpha.permission.C2D_MESSAGE" />


    <!-- Permissions: For VSC Alpha builds -->
    <permission
        android:name="com.expedia.bookings.vsc.alpha.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.expedia.bookings.vsc.alpha.permission.MAPS_RECEIVE" />
    <!-- For Push Notifications (GCM) -->
    <permission
        android:name="com.expedia.bookings.vsc.alpha.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.expedia.bookings.vsc.alpha.permission.C2D_MESSAGE" />

    <application>

        <!-- HockeyApp -->
        <meta-data android:name="buildNumber" android:value="0" />

        <activity
            android:name="net.hockeyapp.android.UpdateActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>

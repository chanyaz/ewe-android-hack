<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.expedia.bookings" >

    <permission
        android:name="com.expedia.bookings.vsc.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.expedia.bookings.vsc.permission.MAPS_RECEIVE" />


    <!-- For Push Notifications (GCM) -->

    <permission
        android:name="com.expedia.bookings.vsc.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.expedia.bookings.vsc.permission.C2D_MESSAGE" />

    <application>

        <receiver
            android:name="com.expedia.bookings.activity.VSCLocaleChangeReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.expedia.bookings.action.vsc_locale_changed" />
            </intent-filter>
        </receiver>

    </application>

</manifest>
